{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dust-tt",
  "title": "Dust.tt",
  "description": "Query dust.tt",
  "icon": "dust.png",
  "author": "vineus",
  "owner": "alan",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ask",
      "title": "Ask Dust",
      "description": "Query dust.tt",
      "mode": "view",
      "arguments": [
        {
          "name": "search",
          "placeholder": "Ask a question",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "explain",
      "title": "Explain",
      "description": "Explain highlighted text",
      "mode": "view"
    },
    {
      "name": "history",
      "title": "Questions history",
      "description": "Show the last 10 questions and answers",
      "mode": "view"
    },
    {
      "name": "askAgent",
      "title": "Ask a Dust agent",
      "description": "Select an agent to ask a question",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.62.2",
    "date-fns": "^3.0.0-alpha.2",
    "eventsource-parser": "^1.1.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
